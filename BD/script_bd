INSERT INTO TB_LOGIN  (SENHA_LOGIN,USUARIO_LOGIN,PESSOA_ID)
VALUES('teste','josemarcos',1);
INSERT INTO TB_LOGIN (SENHA_LOGIN,USUARIO_LOGIN,PESSOA_ID)
VALUES('mario','mario',5);

CREATE TABLE TB_PESSOA (
         id serial PRIMARY KEY,
	 BATIZADO_NBATIZADO VARCHAR (250), 
         CARGO VARCHAR (250), 
         CARGO_ANTERIOR VARCHAR (250), 
         COMENTARIO VARCHAR (250), 
         DATA_NASC DATE, 
         EMPREGADO VARCHAR (250),
         ESTADO_CIVIL VARCHAR (250), 
         IGREJA_ORIGEM VARCHAR (250), 
        NACIONALIDADE VARCHAR (250), 
        NOME VARCHAR (250),
        PROFISSAO VARCHAR (250),	
        REDES_SOCIAL VARCHAR (250), 
      SEXO VARCHAR (10)

);


CREATE TABLE TB_LOGIN (
	id serial PRIMARY KEY,
	SENHA_LOGIN VARCHAR (250),
        USUARIO_LOGIN VARCHAR (250),
        PESSOA_ID INT,
	FOREIGN KEY (PESSOA_ID)
        REFERENCES TB_PESSOA (id)
);






